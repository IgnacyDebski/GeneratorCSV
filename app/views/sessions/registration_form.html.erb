<div style="background-color: gray;">
  <h1 align="center"> Witaj w CSV_Maker! <h1>
  <h1 align="center"> Zarejestruj się :)<h1>
  <a href="https://trello.com">Trello</a>
  <div>
    <%= form_for(:user, :html => {role: "form", url: registration_path }) do |f| %>
    <%= f.text_field :name, placeholder: "Wpisz nazwę użytkownika", autofocus: true %>
    <%= f.email_field :email, placeholder: "Wpisz email" %>
    <%= f.password_field :password, placeholder: "Wpisz hasło", autocomplete: "off" %>
      <p>
        <%= f.submit("Zarejestruj się!") %>
      </p>
      <p align="center">
        <%= link_to "Home", root_path %>
        <p align="right"> <%= link_to "Zaloguj się !", login_path %> </p>
      </p>
    <% end %>
  </div>
  <div> <%= image_tag("Trello.png", style:"width: 50%; height: 100%") %><%= image_tag("csv.png", style:"width: 50%; height: 98%") %> </div>
</div>
